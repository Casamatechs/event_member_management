    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@kasa/koa-logging": "^0.3.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-joi-router": "^6.0.2",
    "koa-joi-router-docs": "^1.2.1",
    "koa2-swagger-ui": "^5.0.3",
    "mysql2": "^2.2.5",
    "pino": "^6.7.0",
    "typeorm": "^0.2.29"
  }
